{
  "name": "code-it",
  "version": "0.1.1-beta.31",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "node dist/server.js",
    "lint": "next lint",
    "db": "json-server -p 5007 -w db.json",
    "preinstall": "prisma generate",
    "prisma:create": "prisma migrate dev --create-only",
    "prisma:db:sync": "prisma db pull",
    "release": "bumpp package.json --commit --push --tag --all"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@prisma/client": "^5.5.2",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/ws": "^8.5.5",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "crypto-js": "^4.2.0",
    "eslint": "8.44.0",
    "eslint-config-next": "^14.0.2",
    "framer-motion": "^10.12.18",
    "jotai": "^1.7.2",
    "json-server": "^0.17.3",
    "monaco-editor": "^0.33.0",
    "monaco-editor-workers": "^0.33.0",
    "monaco-languageclient": "^2.0.2",
    "monaco-vim": "^0.3.4",
    "next": "^14.0.2",
    "nodemon": "^2.0.4",
    "normalize-url": "^7.0.3",
    "postcss": "8.4.24",
    "prisma": "^5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.3",
    "tailwindcss": "3.3.2",
    "ts-node": "^8.10.2",
    "typescript": "5.1.6",
    "usehooks-ts": "^2.15.1",
    "vscode-ws-jsonrpc": "^1.0.1",
    "ws": "^8.13.0",
    "y-monaco": "^0.1.4",
    "y-websocket": "^1.4.5",
    "yjs": "^13.5.45"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "bumpp": "8.2.1"
  }
}
